[Desktop Entry]
Type=Service
MimeType=image/jpeg;image/jpg;
X-KDE-ServiceTypes=KonqPopupMenu/Plugin
X-KDE-Priority=TopLevel
Actions=opt_img_jpg;divide_jpg;conv_to_webp;
Icon=configure
X-KDE-Submenu=Image Tools
X-KDE-Submenu[es]=Herramientas Imagen

[Desktop Action conv_jpg_to_jxl]
Name=Convert JPG to JXL (Lossless)
Name[es]=Covertir JPG a JXL (Sin Pérdida)
Exec=nohup $HOME/.local/share/kio/servicemenus/scripts/conv_jpg_to_jxl.sh %F > /tmp/conv_img.log 2>&1 &
Icon=image-x-generic

[Desktop Action opt_img_jpg]
Name=Optimize images jpg
Name[es]=Optimizar imagenes JPG
Exec=nohup $HOME/.local/share/kio/servicemenus/scripts/compress_jpg.sh %F > /tmp/compl_jpg.log 2>&1 &
Icon=image-x-generic

[Desktop Action divide_jpg]
Name=Divide images(heigh based)
Name[es]=Dividir imágenes(en base a la altura)
Exec=nohup $HOME/.local/share/kio/servicemenus/scripts/divide_jpg.sh %F > /tmp/divide_jpg.log 2>&1 &
Icon=image-x-generic

[Desktop Action conv_to_webp]
Name=Convert to webp
Name[es]=Convertir a WEBP
Exec=nohup $HOME/.local/share/kio/servicemenus/scripts/conv_to_webp.sh %F > /tmp/conv_to_webp.log 2>&1 &
Icon=image-x-generic
